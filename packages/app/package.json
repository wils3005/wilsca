{
  "name": "@wilsjs/app",
  "private": true,
  "scripts": {
    "build": "time (rm -rf dist/ && npm run ts-node -- ./node_modules/webpack/bin/webpack.js)",
    "check": "npm-check --update",
    "eslint": "eslint --format compact --fix --report-unused-disable-directives .",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run",
    "mocha": "nyc mocha --require ts-node/register/transpile-only 'test/**/*.test.ts'",
    "start": "ts-node-dev --require dotenv/config --respawn --transpile-only ./src/server/index.ts",
    "test": "time (npm run eslint && npm run mocha)",
    "ts-node": "ts-node --require dotenv/config --transpile-only"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/hapi": "^20.0.3",
    "@hapi/inert": "6.0.3",
    "@hapi/nes": "12.0.4",
    "aws-sdk": "^2.814.0",
    "bcrypt": "5.0.0",
    "glob": "^7.1.6",
    "hapi-pino": "8.3.0",
    "joi": "^17.3.0",
    "knex": "^0.21.14",
    "objection": "2.2.3",
    "peer": "^0.6.1",
    "pino": "^6.8.0",
    "pino-http": "5.3.0",
    "pino-pretty": "4.3.0",
    "sqlite3": "5.0.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.1",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/hapi-pino": "8.0.1",
    "@types/hapi__basic": "5.1.1",
    "@types/hapi__hapi": "^20.0.3",
    "@types/hapi__inert": "^5.2.2",
    "@types/hapi__nes": "11.0.4",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/mocha": "^8.2.0",
    "@types/pino": "^6.3.4",
    "@types/react-dom": "^17.0.0",
    "@types/sinon": "^9.0.9",
    "@types/webpack-manifest-plugin": "^3.0.1",
    "@types/webpack-node-externals": "^2.5.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "bootstrap": "4.5.3",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "favicons-webpack-plugin": "4.2.0",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "mocha": "^8.2.1",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "webpack": "^5.11.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-node-externals": "^2.5.2"
  }
}
