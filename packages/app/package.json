{
  "name": "@wilsjs/app",
  "private": true,
  "scripts": {
    "build": "npm run ts-node -- ./node_modules/webpack/bin/webpack.js",
    "check": "npm-check --update",
    "eslint": "eslint --format compact --fix --report-unused-disable-directives .",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run",
    "mocha": "nyc mocha --require ts-node/register/transpile-only 'test/**/*.test.ts'",
    "start": "ts-node-dev --require dotenv/config --respawn --transpile-only ./src/index.ts",
    "test": "time (npm run eslint && npm run mocha)",
    "ts-node": "ts-node --require dotenv/config --transpile-only"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/hapi": "20.0.1",
    "@hapi/inert": "6.0.3",
    "@hapi/nes": "12.0.4",
    "@types/glob": "^7.1.3",
    "@types/ws": "7.2.7",
    "aws-sdk": "2.771.0",
    "bcrypt": "5.0.0",
    "glob": "^7.1.6",
    "hapi-pino": "8.3.0",
    "joi": "17.2.1",
    "knex": "0.21.6",
    "objection": "2.2.3",
    "peer": "0.5.3",
    "pino": "6.7.0",
    "pino-http": "5.3.0",
    "pino-pretty": "4.3.0",
    "sqlite3": "5.0.0",
    "uuid": "8.3.1",
    "ws": "7.3.1",
    "zod": "1.11.9"
  },
  "devDependencies": {
    "@types/webpack-node-externals": "^2.5.0",
    "dotenv": "^8.2.0",
    "npm-check": "^5.9.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "webpack": "4.44.2",
    "webpack-node-externals": "^2.5.2"
  }
}
