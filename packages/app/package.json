{
  "name": "@wilsjs/app",
  "private": true,
  "scripts": {
    "build": "time (rm -rf dist/ && npm run ts-node -- ./node_modules/webpack/bin/webpack.js)",
    "check": "npm-check --update",
    "eslint": "eslint --format compact --fix --report-unused-disable-directives .",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run",
    "mocha": "nyc mocha --require ts-node/register/transpile-only 'test/**/*.test.ts'",
    "start": "ts-node-dev --require dotenv/config --respawn --transpile-only ./src/server/index.ts",
    "test": "time (npm run eslint && npm run mocha)",
    "ts-node": "ts-node --require dotenv/config --transpile-only"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/hapi": "20.0.1",
    "@hapi/inert": "6.0.3",
    "@hapi/nes": "12.0.4",
    "@types/glob": "^7.1.3",
    "@types/ws": "7.2.7",
    "aws-sdk": "2.771.0",
    "bcrypt": "5.0.0",
    "glob": "^7.1.6",
    "hapi-pino": "8.3.0",
    "joi": "17.2.1",
    "knex": "0.21.6",
    "objection": "2.2.3",
    "peer": "0.5.3",
    "pino": "6.7.0",
    "pino-http": "5.3.0",
    "pino-pretty": "4.3.0",
    "sqlite3": "5.0.0",
    "uuid": "8.3.1",
    "ws": "7.3.1",
    "zod": "1.11.9"
  },
  "devDependencies": {
    "@types/hapi-pino": "8.0.1",
    "@types/hapi__basic": "5.1.1",
    "@types/hapi__hapi": "20.0.1",
    "@types/hapi__inert": "5.2.1",
    "@types/hapi__nes": "11.0.4",
    "@types/mini-css-extract-plugin": "^1.0.0",
    "@types/mocha": "8.0.3",
    "@types/pino": "6.3.3",
    "@types/react-dom": "16.9.8",
    "@types/sinon": "9.0.8",
    "@types/webpack-manifest-plugin": "2.1.0",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "bootstrap": "4.5.3",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.1.2",
    "favicons-webpack-plugin": "4.2.0",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "^1.1.1",
    "mocha": "8.2.0",
    "npm-check": "^5.9.2",
    "prettier": "2.1.2",
    "react": "16.14.0",
    "react-bootstrap": "1.3.0",
    "react-dom": "16.14.0",
    "sinon": "9.2.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "webpack": "4.44.2",
