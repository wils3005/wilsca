{
  "name": "@wilsjs/app",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "npm run ts-node -- node_modules/webpack/bin/webpack",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:make": "npm run knex -- migrate:make",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run",
    "lint": "eslint --format compact --fix --report-unused-disable-directives .",
    "prestart": "npm run build",
    "start": "node -r dotenv/config ./dist/server/app.js",
    "ts-node": "ts-node --require dotenv/config --transpile-only"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "knex": "^0.21.17",
    "objection": "^2.2.14",
    "pino": "^6.11.0",
    "sqlite3": "^5.0.1",
    "ws": "^7.4.3",
    "zod": "^3.0.0-alpha.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/node": "14.14.22",
    "@types/pino": "^6.3.5",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/uuid": "^8.3.0",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-node-externals": "^2.5.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "css-loader": "^5.0.2",
    "dotenv": "^8.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2",
    "peerjs-js-binarypack": "^1.0.1",
    "phaser": "^3.52.0",
    "pino-pretty": "^4.5.0",
    "pnpm": "^5.17.2",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "4.1.3",
    "uuid": "^8.3.2",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  }
}
