{
  "name": "@wilsjs/server",
  "private": true,
  "scripts": {
    "check": "npx npm-check --update-all",
    "docker": "time docker build -t wilsjs-server .",
    "eslint": "npx eslint .",
    "eslint-fix": "npx eslint --fix .",
    "knex": "node --require dotenv/config --require ts-node/register node_modules/knex/bin/cli.js --knexfile knexfile.ts",
    "knex-migrate": "npm run knex -- migrate:latest",
    "knex-seed": "npm run knex -- seed:run",
    "prettier": "npx prettier --list-different .",
    "prettier-write": "npx prettier --write .",
    "start": "node ../../node_modules/ts-node-dev/bin/ts-node-dev --require dotenv/config --respawn --transpile-only src/index.ts",
    "build": "npx webpack --verbose"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/hapi": "20.0.1",
    "@hapi/inert": "6.0.2",
    "@hapi/nes": "12.0.3",
    "@hapi/vision": "6.0.1",
    "@types/ws": "7.2.6",
    "aws-sdk": "2.759.0",
    "bcrypt": "5.0.0",
    "dotenv": "8.2.0",
    "hapi-pino": "8.3.0",
    "joi": "17.2.1",
    "knex": "0.21.5",
    "objection": "2.2.3",
    "peer": "0.5.3",
    "pino": "6.6.1",
    "pino-http": "5.3.0",
    "pino-pretty": "4.2.1",
    "schwifty": "5.4.0",
    "sqlite3": "5.0.0",
    "tozod": "0.0.1",
    "uuid": "8.3.0",
    "ws": "7.3.1",
    "zod": "1.11.9"
  }
}
