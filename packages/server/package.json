{
  "name": "@wilsjs/server",
  "private": true,
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json",
    "check": "npm-check --update-all",
    "knex": "node --require dotenv/config --require ts-node/register ./node_modules/knex/bin/cli.js",
    "knex-migrate": "npm run knex -- migrate:latest",
    "knex-seed": "npm run knex -- seed:run",
    "repl": "ts-node --require dotenv/config --transpile-only ./repl.ts",
    "start": "node ../../node_modules/ts-node-dev/bin/ts-node-dev --require dotenv/config --respawn --transpile-only ./src/index.ts"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/hapi": "20.0.1",
    "@hapi/inert": "6.0.3",
    "@hapi/nes": "12.0.4",
    "@hapi/vision": "6.0.1",
    "@types/ws": "7.2.7",
    "aws-sdk": "2.771.0",
    "bcrypt": "5.0.0",
    "hapi-pino": "8.3.0",
    "joi": "17.2.1",
    "knex": "0.21.6",
    "objection": "2.2.3",
    "peer": "0.5.3",
    "pino": "6.7.0",
    "pino-http": "5.3.0",
    "pino-pretty": "4.3.0",
    "sqlite3": "5.0.0",
    "uuid": "8.3.1",
    "ws": "7.3.1",
    "zod": "1.11.9"
  }
}
