{
  "name": "@wilsjs/server",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "tsc --build tsconfig.build.json",
    "lint": "eslint --format compact --fix --report-unused-disable-directives .",
    "start": "ts-node-dev --require dotenv/config --respawn --transpile-only src/index.ts",
    "ts-node": "ts-node --require dotenv/config --transpile-only",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:make": "npm run knex -- migrate:make",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run"
  },
  "dependencies": {
    "colyseus": "^0.14.7",
    "express-pino-logger": "^6.0.0",
    "knex": "^0.21.17",
    "objection": "^2.2.14",
    "peer": "^0.6.1",
    "pino": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.1",
    "ws": "^7.4.2",
    "zod": "^3.0.0-alpha.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/node": "14.14.22",
    "@types/pino": "^6.3.5",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "dotenv": "^8.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "express": "^4.17.1",
    "pnpm": "^5.16.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "4.1.3"
  }
}
