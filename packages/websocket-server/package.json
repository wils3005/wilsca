{
  "name": "websocket-server",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run clean && ../../node_modules/.bin/tsc --build tsconfig.build.json",
    "check": "../../node_modules/.bin/npm-check -u",
    "clean": "rm -rf build/",
    "docker-build": "npm run build && docker build --tag flumpjs-websocket-server .",
    "docker-run": "docker run --detach --env-file .env --name flumpjs-websocket-server-1 --publish 3003:3003 flumpjs-websocket-server",
    "start": "../../node_modules/.bin/ts-node-dev --require dotenv/config --respawn --transpileOnly src/index.ts"
  },
  "dependencies": {
    "pino": "6.3.2",
    "ws": "7.3.0"
  }
}
