{
  "name": "@wilsjs/hapi-server",
  "private": true,
  "scripts": {
    "build": "tsc --build ./src/tsconfig.json",
    "check": "npm-check --update",
    "eslint": "eslint --format compact --fix --report-unused-disable-directives .",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run",
    "mocha": "nyc mocha --require ts-node/register/transpile-only 'test/**/*.test.ts'",
    "start": "ts-node-dev --require dotenv/config --respawn --transpile-only ./src/index.ts",
    "test": "npm run eslint && npm run mocha",
    "ts-node": "ts-node --require dotenv/config --transpile-only"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/hapi": "19.x.x",
    "@hapi/inert": "6.0.3",
    "@hapi/nes": "12.0.4",
    "aws-sdk": "^2.819.0",
    "bcrypt": "5.0.0",
    "glob": "^7.1.6",
    "hapi-pino": "8.3.0",
    "joi": "^17.3.0",
    "knex": "^0.21.15",
    "objection": "^2.2.4",
    "peer": "^0.6.1",
    "pino": "^6.9.0",
    "pino-http": "5.3.0",
    "pino-pretty": "4.3.0",
    "sqlite3": "5.0.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.2",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/hapi__basic": "5.1.1",
    "@types/hapi__hapi": "19.x.x",
    "@types/hapi__inert": "^5.2.2",
    "@types/hapi__nes": "11.0.4",
    "@types/hapi-pino": "8.0.1",
    "@types/mocha": "^8.2.0",
    "@types/pino": "^6.3.4",
    "@types/sinon": "^9.0.10",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "bootstrap": "4.5.3",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  }
}
