{
  "name": "@wilsjs/flump",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "npm run ts-node -- node_modules/webpack/bin/webpack",
    "check": "npm-check --update",
    "knex": "npm run ts-node -- ./node_modules/knex/bin/cli.js",
    "knex:make": "npm run knex -- migrate:make",
    "knex:migrate": "npm run knex -- migrate:latest",
    "knex:seed": "npm run knex -- seed:run",
    "lint": "eslint --format compact --fix --report-unused-disable-directives .",
    "prestart": "npm run build",
    "start": "node -r dotenv/config ./dist/server/app.js",
    "ts-node": "ts-node --require dotenv/config --transpile-only"
  },
  "dependencies": {
    "fastify": "^3.14.0",
    "fastify-jwt": "^2.4.0",
    "fastify-openapi-glue": "^2.4.0",
    "fastify-static": "^4.0.1",
    "fastify-swagger": "^4.4.2",
    "fastify-websocket": "^3.1.0",
    "knex": "^0.21.19",
    "pino": "^6.11.0",
    "sqlite3": "^5.0.1",
    "zod": "^3.0.0-alpha.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/pino": "^6.3.5",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/uuid": "^8.3.0",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-node-externals": "^2.5.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2",
    "phaser": "^3.52.0",
    "pino-pretty": "^4.5.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2",
    "webrtc-adapter": "^7.7.0"
  }
}
