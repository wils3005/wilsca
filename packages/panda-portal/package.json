{
  "name": "@wilsjs/panda-portal",
  "private": true,
  "scripts": {
    "build": "rm -rf build/ && tsc --build ./tsconfig.build.json",
    "server": "ts-node-dev --require dotenv/config --respawn --transpile-only src/server/index.ts",
    "eslint": "eslint .",
    "eslint-fix": "eslint --fix .",
    "jest": "jest",
    "prettier": "prettier --list-different .",
    "prettier-write": "prettier --write .",
    "webpack-production": "webpack --mode production",
    "webpack-watch": "webpack --watch"
  },
  "dependencies": {
    "express": "4.17.1",
    "pino": "6.5.0",
    "sqlite3": "5.0.0",
    "uuid": "8.3.0",
    "ws": "7.3.1"
  }
}
