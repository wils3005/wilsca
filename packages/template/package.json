{
  "name": "@wilsjs/template",
  "private": true,
  "scripts": {
    "build": "npm run webpack -- --mode production",
    "check": "npx npm-check --update-all",
    "docker": "docker build --tag wilsjs-template .",
    "eslint": "npx eslint .",
    "eslint-fix": "npx eslint --fix .",
    "jest": "npx jest --forceExit",
    "knex": "node --require dotenv/config --require ts-node/register node_modules/.bin/knex --knexfile knexfile.ts",
    "knex-migrate": "npm run knex -- migrate:latest",
    "knex-seed": "npm run knex -- seed:run",
    "prettier": "npx prettier --list-different .",
    "prettier-write": "npx prettier --write .",
    "start": "npx ts-node-dev --require dotenv/config --respawn --transpile-only src/server/index.ts",
    "webpack": "webpack"
  },
  "dependencies": {
    "aws-sdk": "2.738.0",
    "bcrypt": "5.0.0",
    "connect-session-knex": "1.7.3",
