{
  "name": "@wilsjs/core",
  "private": true,
  "scripts": {
    "check": "npx npm-check --update-all",
    "docker": "time docker build -t wilsjs-core .",
    "eslint": "npx eslint .",
    "eslint-fix": "npx eslint --fix .",
    "knex": "node --require dotenv/config --require ts-node/register node_modules/knex/bin/cli.js --knexfile knexfile.ts",
    "knex-migrate": "npm run knex -- migrate:latest",
    "knex-seed": "npm run knex -- seed:run",
    "prettier": "npx prettier --list-different .",
    "prettier-write": "npx prettier --write .",
    "start": "npx ts-node-dev --require dotenv/config --respawn --transpile-only src/server/index.ts",
    "ssh": "ssh -i wilsca.pem alpine@wils.ca",
    "test": "npx mocha 'src/**/*.test.ts'",
    "tsc": "npm run tsc-client && npm run tsc-server",
    "tsc-client": "npx tsc --build tsconfig.client.json",
    "tsc-server": "npx tsc --build tsconfig.server.json"
  },
