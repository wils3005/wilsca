{
  "name": "peer-server",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run clean && ../../node_modules/.bin/tsc --build tsconfig.build.json",
    "check": "../../node_modules/.bin/npm-check -u",
    "clean": "rm -rf build/",
    "docker-build": "npm run build && docker build --tag flumpjs-peer-server .",
    "docker-run": "docker run --detach --env-file .env --name flumpjs-peer-server-1 --publish 3002:3002 flumpjs-peer-server",
    "start": "../../node_modules/.bin/ts-node-dev --require dotenv/config --respawn --transpileOnly src/index.ts"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-pino-logger": "5.0.0",
    "peer": "0.5.3",
    "pino": "6.3.2"
  }
}
