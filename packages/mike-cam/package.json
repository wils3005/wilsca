{
  "name": "mike-cam",
  "scripts": {
    "webpack": "../../node_modules/.bin/webpack --mode production",
    "build": "rm -rf build/ && ../../node_modules/.bin/tsc --build tsconfig.build.json",
    "check": "../../node_modules/.bin/npm-check -u",
    "docker-build": "npm run build && docker build --tag mike-cam .",
    "docker-run": "docker run --detach --env-file .env --name mike-cam-1 --publish 3002:3002 mike-cam",
    "watch": "../../node_modules/.bin/webpack --watch",
    "start": "../../node_modules/.bin/ts-node-dev --require dotenv/config --respawn --transpileOnly src/index.ts"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-pino-logger": "5.0.0",
    "peer": "0.5.3",
    "pino": "6.3.2"
  }
}
