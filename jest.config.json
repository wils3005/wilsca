{
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*.ts", "!**/*.test.ts"],
  "coverageDirectory": "coverage",
  "coverageReporters": ["html", "text"],
  "coverageThreshold": {
    "global": {
      "branches": 1,
      "functions": 1,
      "lines": 1,
      "statements": 1
    }
  },
  "preset": "ts-jest",
  "testEnvironment": "node"
}
