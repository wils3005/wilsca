{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "commonjs",
    "outDir": "build/",
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true
  }
}
