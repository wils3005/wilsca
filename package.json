{
  "name": "flumpjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-client": "node node_modules/.bin/webpack --mode production",
    "check": "node_modules/.bin/npm-check -u",
    "client": "node node_modules/.bin/webpack --watch",
    "deploy": "git fetch && git reset --hard origin/master && npm run tsc && systemctl restart flumpjs && npm run build-client && cp public/* /home/nginx/",
    "server": "node_modules/.bin/ts-node-dev --require dotenv/config --respawn --transpileOnly ./server/index.ts",
    "test": "NODE_ENV=test node_modules/.bin/eslint '**/*.ts' && node_modules/.bin/jest --passWithNoTests",
    "tsc": "node_modules/.bin/tsc --build"
  },
  "dependencies": {
    "@colyseus/monitor": "0.12.2",
    "colyseus": "0.13.7",
    "express": "4.17.1",
    "express-basic-auth": "1.2.0",
    "express-jwt": "5.3.3",
    "express-pino-logger": "5.0.0",
    "objection": "2.2.0",
    "peer": "0.5.3",
    "pg": "8.2.1",
    "pino": "6.3.2"
  },
  "devDependencies": {
    "@material-ui/core": "4.10.1",
    "@types/express": "4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/express-pino-logger": "4.0.2",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/peerjs": "1.1.0",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "dotenv-webpack": "1.8.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "jest": "26.0.1",
    "npm-check": "5.9.2",
    "peerjs": "1.2.0",
    "phaser": "3.23.0",
    "pino-pretty": "4.0.0",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "style-loader": "1.2.1",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.44",
    "type-fest": "0.15.1",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  }
